!function(){function e(){t&&clearTimeout(t),t=setTimeout(function(){var t=l.val().trim();if(""==(t=t.replace(/[^a-zA-Z0-9\s]/g," ").trim()))return!1;var e=function(t){return(t=(t=(t=t.toLowerCase()).replace(/[-_]/g," ")).replace(/(?:^\w|[A-Z]|\b\w)/g,function(t,e){return 0===e?t.toLowerCase():t.toUpperCase()})).replace(/\s+/g,"")}(t),a=function(t){return(t=(t=(t=t.toLowerCase()).replace(/[-_]/g," ")).replace(/(?:^\w|[A-Z]|\b\w)/g,function(t,e){return t.toUpperCase()})).replace(/\s+/g,"")}(t),r=function(t){return t=(t=t.replace(/\s+/g,"_")).toLowerCase()}(t),n=function(t){return t=(t=t.replace(/\s+/g,"_")).toUpperCase()}(t),o=function(t){return t=t.replace(/\s+/g,".")}(t),c=function(t){return t=(t=t.replace(/\s+/g,"-")).toLowerCase()}(t);$("#text-camel-lcfrist").val(e).attr("data-clipboard-text",e),$("#text-camel-ucfrist").val(a).attr("data-clipboard-text",a),$("#text-unline-lower").val(r).attr("data-clipboard-text",r),$("#text-unline-upper").val(n).attr("data-clipboard-text",n),$("#text-package-name").val(o).attr("data-clipboard-text",o),$("#text-mhline-lower").val(c).attr("data-clipboard-text",c)},250)}var l=$("#tool-text"),t=null;l.bind("paste",e),l.bind("keyup",e),l.bind("blur",e),l.bind("change",e),$(".btn-reset").click(function(){$(".tool-form .form-input").val("").attr("data-clipboard-text","")}),$("#btn-paste").click(function(){try{navigator.clipboard.readText().then(function(t){l.val(t),e()})}catch(t){console.log(t.name,t.message)}}),new Clipboard(".convert-panel .form-input").on("success",function(t){var e=t.trigger.value;t.trigger.value="已复制",setTimeout(function(){t.trigger.value=e},1e3)})}();