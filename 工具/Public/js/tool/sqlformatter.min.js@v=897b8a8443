function sql_formatter(t){var s=$("#tool-text").val();if(""==s)return tips_error("请填写要转换的SQL语句！"),$("#tool-text").focus(),!1;var e=$(".btn-success");e.attr("disabled","disabled"),tips_loading(),$.ajax({type:"post",dataType:"json",data:{type:t,text:s},url:WW_URL+"ajax",success:function(t){if(1==t.status){tips_clear();var s=t.data;$("#tool-result").html(s),$("#tool-result").show()}else tips_error(t.info),$("#tool-text").focus();e.removeAttr("disabled","disabled")}})}