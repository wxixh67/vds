var sentence="#sentence";function change(){var t=$("#change");t.attr("disabled","disabled"),$.ajax({type:"GET",url:WW_URL+"ajax_test",dataType:"json",success:function(e){return 1==e.status?($(sentence).html(e.data),$(".btn-copyit").attr("data-clipboard-text",e.data),fitty(sentence,{minSize:22,maxSize:40}),setTimeout(function(){t.removeAttr("disabled","disabled")},2e3),!0):($(sentence).html(e.info),!1)}})}fitty(sentence,{minSize:22,maxSize:40});var clipboard_copyit=new Clipboard(".btn-copyit");clipboard_copyit.on("success",function(e){var t=e.trigger.value;e.trigger.value="已复制",setTimeout(function(){e.trigger.value=t},2e3)}),clipboard_copyit.on("error",function(e){alert("复制失败，请手动操作。")});