function generator(){var t=$("input[name='expression']").val();if(""==t)return tips_error("请填写crontab表达式！"),$("#tool-text").focus(),!1;var s=$(".btn-success");s.attr("disabled","disabled"),tips_loading(),$.ajax({type:"post",dataType:"json",data:{expression:t},url:WW_URL+"ajax",success:function(t){if(1==t.status){for(var a in tips_clear(),html="未来10次执行时间：<br/><ol>",t.data)html+="<li>"+t.data[a]+"</li>";html+="</ol>",$("#tool-result").html(html),$("#tool-result").show()}else tips_error(t.info);s.removeAttr("disabled","disabled")}})}