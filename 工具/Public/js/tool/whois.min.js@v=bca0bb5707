function whois_search(){if(""==(t=$.trim($("#tool-text").val().toLowerCase().replace(/\s+/g,""))))return tips_error("请填写要查询的域名！"),$("#tool-text").focus(),!1;if(t=t.match(/^(https?:\/\/)?([-a-z0-9\.]+)\/?/)){$("#tool-text").val(t[2]);var t=t[2]}var e=$(".btn-success");e.attr("disabled","disabled"),tips_loading(),$("#tool-result").hide(),$.ajax({type:"post",dataType:"json",headers:{"X-CSRF-Token":getCSRFToken()},data:{domain:t},url:WW_URL+"ajax",success:function(t){1==t.status?(tips_clear(),$("#tool-result").html("<pre>"+t.data+"</pre>"),$("#tool-result").show()):(tips_error(t.info),$("#tool-text").focus()),e.removeAttr("disabled","disabled")}})}