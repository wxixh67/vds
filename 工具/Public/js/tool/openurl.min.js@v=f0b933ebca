var urlArray=new Array,openInterval=300,localKey="wawa_local_open_urls",text_id=$("#tool-form");function showCount(){var t=text_id.val(),e=0;if(0==t.Length)$("#url-count").html(e);else{for(var o=0;-1!=(o=t.indexOf("\n",o+1));)e++;$("#url-count").html(e+1)}}function openUrl(){if(0==urlArray.length)return $("#btn-openurl").removeAttr("disabled","disabled"),!1;var t=window.open(),e=urlArray.shift();t.location.href=e,setTimeout(function(){openUrl()},openInterval)}$(document).ready(function(){var t=localStorage.getItem(localKey);null!=t&&(text_id.val(t),showCount()),text_id.bind("keyup change",function(){showCount(),""!=text_id.val()?localStorage.setItem(localKey,text_id.val()):localStorage.removeItem(localKey)}),$("#btn-openurl").click(function(){$(this).attr("disabled","disabled");for(var t=text_id.val().split("\n"),e=0;e<t.length;e++){var o=t[e].replace(/(^\s*)(\s*$)/g,"");""!=o&&0!=o.indexOf("#")&&(0!=o.toLowerCase().indexOf("http://")&&0!=o.toLowerCase().indexOf("https://")&&(o="http://"+o),urlArray.push(o))}setTimeout(function(){openUrl()},openInterval)})});