$(document).ready(function(){$("#btn-submit").click(function(){var t=$("#tool-form").val();if(""==t)return tips_error("请填写需要加密的内容！"),!1;tips_loading(),$.ajax({type:"post",dataType:"json",data:{str:t},url:WW_URL+"ajax",success:function(t){if(1!=t.status)return $("#tool-result").hide(),tips_error(t.info),!1;tips_clear(),$("#tool-result").show();for(var r=t.data,a="<table><tbody>",e=0;e<r.length;e++)a+='<tr><td style="padding:3px 10px 3px 0;"><span class="label label-warning">'+r[e].name+'</span></td><td style="padding:3px 10px 3px 0;">'+r[e].value+'</td><td><span class="label label-default btn-copyit" data-clipboard-text="'+r[e].value+'">复制</span></td></tr>';a+="</tbody></table>",$("#tool-result").html(a)}})});var t=new Clipboard(".btn-copyit");t.on("success",function(t){var r=t.trigger.innerHTML;t.trigger.innerHTML="已复制",setTimeout(function(){t.trigger.innerHTML=r},3e3)}),t.on("error",function(t){tips_error("复制失败，请手动操作。")})});