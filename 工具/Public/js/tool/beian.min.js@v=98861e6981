function beian(){var t=$.trim($("#tool-text").val().toLowerCase());if(""==t)return tips_error("请填写要查询的域名！"),$("#tool-text").focus(),!1;var e=t.match(/^(https?:\/\/)?([-a-z0-9\.]+)\/?/);if(!e)return tips_error("请填写正确的域名！"),$("#tool-text").focus(),!1;$("#tool-text").val(e[2]);var o=e[2],a=$(".btn-success");a.attr("disabled","disabled"),tips_loading(),$("#tool-result").hide(),$.ajax({type:"post",dataType:"json",headers:{"X-CSRF-Token":getCSRFToken()},data:{domain:o},url:WW_URL+"ajax",success:function(t){1==t.status?(tips_clear(),$("#tool-result").html("<pre>"+t.data+"</pre>"),$("#tool-result").show()):(tips_error(t.info),$("#tool-text").focus()),a.removeAttr("disabled","disabled")}})}