function daxue(){var t=$("select[name='type']").val(),a=$("input[name='keyword']").val();if(""==t)return tips_error("请选择查询类型！"),!1;if(""==a)return tips_error("请填写关键词！"),!1;var s=$(".btn-success");s.attr("disabled","disabled"),tips_loading(),$.ajax({type:"post",dataType:"json",data:{type:t,keyword:a},url:WW_URL+"ajax",success:function(t){if(1==t.status){tips_clear(),data_list=t.data;for(var a="<div class='table-scroll-x'><table class='table'><thead><tr><th>序列</th><th>院校名称</th><th>院校标识码</th><th>主管部门</th><th>所在地</th><th>办学层次</th><th>类型</th><th>等级</th></tr></thead><tbody>",l=0;l<data_list.length;l++)a+="<tr><td>"+data_list[l].id+"</td><td>"+data_list[l].name+"</td><td>"+data_list[l].code+"</td><td>"+data_list[l].department+"</td><td>"+data_list[l].city+"</td> <td>"+data_list[l].level+"</td>",null==data_list[l].type?a+="<td>-</td>":a+="<td>"+data_list[l].type+"</td>",a+="<td>",1==data_list[l].top985&&(a+='<span class="label label-danger">985</span> '),1==data_list[l].top211&&(a+='<span class="label label-warning">211</span> '),1==data_list[l].shuangyiliu&&(a+='<span class="label label-success">双一流</span>'),0==data_list[l].top985&&0==data_list[l].top211&&0==data_list[l].shuangyiliu&&(a+="-"),a+="</td></tr>";a+="</tbody></table></div>",$("#tool-result").show(),$("#tool-result").html(a)}else $("#tool-result").hide(),tips_error(t.info);s.removeAttr("disabled","disabled")}})}