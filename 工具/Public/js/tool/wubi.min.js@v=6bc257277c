function wubi(){var t=$.trim($("#tool-text").val());if(""==t)return tips_error("请填写要查询的汉字！"),$("#tool-text").focus(),!1;if(!/^[\u4e00-\u9fa5]{1,10}$/.test(t))return tips_error("每次查询最多10个汉字！请不要包含空格等无用符号。"),$("#tool-text").focus(),!1;var d=$(".btn-success");d.attr("disabled","disabled"),tips_loading(),$.ajax({type:"post",dataType:"json",data:{hanzi:t},url:WW_URL+"ajax",success:function(t){if(1==t.status){tips_clear(),data_list=t.data;for(var a="<table class='table' width='100%'> <thead><tr><th>汉字</th><th>86编码</th><th>86简码</th><th>98编码</th><th>98简码</th><th>拆字图</th><th>拼音</th><th>笔画数</th></tr></thead><tbody>",r=0;r<data_list.length;r++)a+="<tr><td>"+data_list[r].hanzi+"</td><td>"+data_list[r].c86+"</td><td>"+data_list[r].c86j+"</td><td>"+data_list[r].c98+"</td><td>"+data_list[r].c98j+"</td><td><img src='/Data/wubi/"+data_list[r].hanzi+".png' height='40' onerror='img_error();' /></td><td>"+data_list[r].py+"</td><td>"+data_list[r].bh+"</td></tr>";a+="</tbody></table>",$("#tool-result").html(a),$("#tool-result").show()}else tips_error(t.info),$("#tool-text").focus();d.removeAttr("disabled","disabled")}})}function img_error(){var t=event.srcElement;t.src="/Data/wubi/error.png",t.onerror=null}