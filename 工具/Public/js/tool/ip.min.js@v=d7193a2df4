function ip_search(){if(""==(e=$.trim($("#tool-text").val().toLowerCase().replace(/\s+/g,""))))return tips_error("请填写要查询的IP或域名！"),$("#tool-text").focus(),!1;var t=e.match(/^(https?:\/\/)?([-a-z0-9\.]+)\/?/);if(t){$("#tool-text").val(t[2]);var e=t[2]}var a=$(".btn-success");a.attr("disabled","disabled"),tips_loading(),$.ajax({type:"post",dataType:"json",headers:{"X-CSRF-Token":getCSRFToken()},data:{ip:e},url:WW_URL+"ajax_test",success:function(t){1==t.status?(tips_clear(),$("#tool-result").html(t.data),$("#tool-result").show()):(tips_error(t.info),$("#tool-text").focus()),a.removeAttr("disabled","disabled")}})}