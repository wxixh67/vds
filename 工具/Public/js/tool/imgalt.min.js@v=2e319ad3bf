function replace_alt(){var t=$.trim($("#tool-form").val());if(""==t)return tips_error("请填写需要处理的HTML代码！"),$("#tool-form").focus(),!1;var a=$("#img_alt").val(),i=$("#img_title").val(),r=$("#img_width").val();if(""==a)return tips_error("请填写ALT标签内容！"),$("#img_alt").focus(),!1;var l=$(".btn-success");l.attr("disabled","disabled"),tips_loading(),$.ajax({type:"post",dataType:"json",data:{str:t,img_alt:a,img_title:i,img_width:r},url:WW_URL+"ajax",success:function(t){1==t.status?(tips_clear(),$("#tool-result").val(t.data)):(tips_error(t.info),$("#tool-text").focus()),l.removeAttr("disabled","disabled")}})}