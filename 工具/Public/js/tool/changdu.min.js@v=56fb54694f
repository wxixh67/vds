!function(){$(".exchange-panel");function n(){var n=this;a&&clearTimeout(a),a=setTimeout(function(){var a=n.value.trim(),o=n.name;return(o!=r||a!=i)&&(""==o?(tips_error("请选择转换前单位！"),!1):""==a?(tips_error("请填写长度！"),!1):(tips_loading(),void $.ajax({type:"post",dataType:"json",data:{from:o,to:"all",length:a},url:WW_URL+"ajax",success:function(n){if(1!=n.status)return tips_error(n.info),!1;tips_clear(),i=a,r=o;for(var t=n.data,e=0;e<t.length;e++)$('input[name="'+t[e].unit+'"]').val(t[e].value)}})))},500)}for(var t,e=$(".form-input"),a=null,r=null,i=null,o=0;o<e.length;o++)e[t=o].onkeydown=function(){a&&clearTimeout(a)},e[t].onkeyup=n,e[t].onchange=n,e[t].onclick=function(){$(this).select()}}();