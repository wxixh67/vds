function riqidaxie(){var t=$("#tool-text").val();if(""==t)return tips_error("请填写您要转换的日期！"),$("#tool-text").focus(),!1;var a=$(".btn-success");a.attr("disabled","disabled"),tips_loading(),$.ajax({type:"post",dataType:"json",data:{date:t},url:WW_URL+"ajax",success:function(t){if(1==t.status){tips_clear();var e="<em>"+t.data.year+"</em> 年 <em>"+t.data.month+"</em> 月 <em>"+t.data.day+"</em> 日";$("#tool-result").show(),$("#tool-result").html(e)}else $("#tool-result").hide(),tips_error(t.info);a.removeAttr("disabled","disabled")}})}$("#tool-text").cxCalendar({startDate:"2000-01-01",endDate:"2999-12-31"});