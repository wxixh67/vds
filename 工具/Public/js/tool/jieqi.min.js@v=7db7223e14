function jieqi(){var t=$("#tool-text").val();if(""==t)return tips_error("请填写您要查询的公历年份！"),$("#tool-text").focus(),!1;if(t<0||3e3<t)return tips_error("仅支持公历0~3000年！"),$("#tool-text").focus(),!1;var r=p.GetAdjustedJQ(t-1,!1),a=p.GetAdjustedJQ(t,!1),d='<div class="table-scroll-x"><table class="table"><thead><tr><th width="120">节气</th><th>公历日期</th><th>农历日期</th><th>时间</th></tr></thead><tbody>';for(var o in r){(e=p.Jtime(r[o]))[0]==t&&(d+="<tr><td>"+p.jq[parseInt(o)%24]+"</td><td>"+e[0]+"年"+e[1].toString().padStart(2,"0")+"月"+e[2].toString().padStart(2,"0")+"日</td><td>"+formatLunarDate(e[0],e[1],e[2])+"</td><td>"+e[3].toString().padStart(2,"0")+"时"+e[4].toString().padStart(2,"0")+"分"+e[5].toString().padStart(2,"0")+"秒</td></tr>")}for(var o in a){var e;(e=p.Jtime(a[o]))[0]==t&&(d+="<tr><td>"+p.jq[parseInt(o)%24]+"</td><td>"+e[0]+"年"+e[1].toString().padStart(2,"0")+"月"+e[2].toString().padStart(2,"0")+"日</td><td>"+formatLunarDate(e[0],e[1],e[2])+"</td><td>"+e[3].toString().padStart(2,"0")+"时"+e[4].toString().padStart(2,"0")+"分"+e[5].toString().padStart(2,"0")+"秒</td></tr>")}d+="</tbody></table></div>",tips_clear(),$("#tool-result").html(d),$("#tool-result").show()}function formatLunarDate(t,r,a){var d=p.Solar2Lunar(t,r,a);return d[0]+"年"+p.dxy[d[1]-1]+p.dxd[d[2]-1]}