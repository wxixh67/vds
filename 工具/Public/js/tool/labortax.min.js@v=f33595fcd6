function get_tax(){var e=parseFloat($("input[name='money']").val());if(isNaN(e))return tips_error("请填写收入金额！"),$("input[name='money']").focus(),$("input[name='money']").select(),$("#tool-result").hide(),!1;if($("input[name='money']").val(e),e<=800)return tips_error("收入低于800元，无需纳税。"),!1;var t=800;4e3<e&&(t=.2*e);var r=e-t,o=.2,i=0;2e4<r&&r<=5e4?(o=.3,i=2e3):5e4<r&&(o=.4,i=7e3);var n=r*o-i,a=e-n,l="";l+=n<=0?"无需纳税！":"扣除金额："+t.toFixed(2)+"元<br/>应纳税所得额 ："+r.toFixed(2)+"元<br/>适用税率："+100*o+"%<br/>速算扣除数："+i.toFixed(2)+"元<br/>应缴税款："+n.toFixed(2)+"元<br/>税后收入："+a.toFixed(2)+"元",tips_clear(),$("#tool-result").show(),$("#tool-result").html(l)}