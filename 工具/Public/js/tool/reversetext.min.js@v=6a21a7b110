function flipString(r){for(var t=r.length-1,e=new Array(r.length),i=t;0<=i;--i){var l=r.charAt(i),n=flipTable[l];e[t-i]=n||l}return e.join("")}$(function(){$("#btn-reverse").click(function(){var r=$.trim($("#tool-text").val()),t=$("#split_format").val();if(""==r)return tips_error("请填写需要反转排序的文本！每一行为一个项目。"),$("#tool-text").focus(),!1;0==t?(re=/[\r\n]/g,rb="\n"):1==t?(re=/[^\S\r\n]/g,rb=" "):2==t?(re=",",rb=","):3==t?(re="，",rb="，"):4==t&&(re=/[\/]/g,rb="/");var e=r.split(re).reverse(),i="",l=0;$.each(e,function(r,t){i+=0==l?t.trim():rb+t.trim(),l+=1}),tips_clear(),$("#tool-result").val(i)}),$("#btn-flip").click(function(){var r=$.trim($("#tool-text").val());if(""==r)return tips_error("请填写需要反转排序的文本！"),$("#tool-text").focus(),!1;var t=flipString(r);tips_clear(),$("#tool-result").val(t)})});var flipTable={a:"ɐ",b:"q",c:"ɔ",d:"p",e:"ǝ",f:"ɟ",g:"ƃ",h:"ɥ",i:"ı",j:"ɾ",k:"ʞ",m:"ɯ",n:"u",r:"ɹ",t:"ʇ",v:"ʌ",w:"ʍ",y:"ʎ",".":"˙","[":"]","(":")","{":"}","?":"¿","!":"¡","'":",","<":">",_:"‾","‿":"⁀","⁅":"⁆","∴":"∵","\r":"\n"};for(i in flipTable)flipTable[flipTable[i]]=i;