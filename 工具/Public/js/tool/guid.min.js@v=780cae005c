function get_guid(){var e=parseInt($("input[name='number']").val()),t=$("input[name='upper']:checked").val(),a=$("input[name='hyphen']:checked").val();if(e<1||20<e)return tips_error("生成个数必须在1~20间"),!1;var r=$(".btn-success");r.attr("disabled","disabled"),tips_loading(),$.ajax({type:"post",dataType:"json",data:{number:e,upper:t,hyphen:a},url:WW_URL+"ajax",success:function(e){1==e.status?(tips_clear(),$("#tool-result").html(e.data),$("#tool-result").show(),$(".btn-copy").trigger("click")):tips_error("生成失败！请重试。"),r.removeAttr("disabled","disabled")}})}