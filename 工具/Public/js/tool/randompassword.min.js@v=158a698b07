function generator(){var a=$(".btn-success");a.attr("disabled","disabled"),tips_loading(),$.ajax({type:"post",dataType:"json",data:{lower:$("input[name='lower']").is(":checked"),upper:$("input[name='upper']").is(":checked"),num:$("input[name='num']").is(":checked"),sym:$("input[name='sym']").is(":checked"),length:$("input[name='length']").val(),prefix:$("input[name='prefix']").val(),suffix:$("input[name='suffix']").val()},url:WW_URL+"ajax",success:function(e){1==e.status?(tips_clear(),html="密码: "+e.pw+"<br/>MD5: "+e.md5,$("input[name='pw']").val(e.pw),$("input[name='md5']").val(e.md5)):tips_error(e.info),a.removeAttr("disabled","disabled")}})}