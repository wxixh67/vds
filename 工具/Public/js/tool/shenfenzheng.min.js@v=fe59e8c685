function shenfenzheng(){$("#tool-result").hide();var t=new IDValidator(GB2260),r=$("#tool-text").val();if(""==r)return tips_error("请输入完整的身份证号！"),$("#tool-text").focus(),!1;if(0==t.isValid(r))return tips_error("身份证号有误，请输入正确的15位或18位身份证号！"),$("#tool-text").focus(),!1;tips_clear();var o=t.getInfo(r),e="";e+="地址："+o.addr.toString(),e+="</br>生日："+o.birth,1==o.sex?e+="</br>性别：男":e+="</br>性别：女",$("#tool-result").html(e),$("#tool-result").show()}