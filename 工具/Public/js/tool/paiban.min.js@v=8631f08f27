function text_correct(t){return(new CopyWritingCorrectService).correct(t)}$(function(){$("#btn-correct").click(function(){var t=$("#tool-text").val();if(t.length<=0)return tips_error("请输入要纠正排版的内容！"),$("#tool-text").focus(),!1;tips_clear(),$("#tool-result").val(text_correct(t))}),$("#tool-text").bind("input",function(){var t=$("#tool-text").val();$("#tool-result").val(text_correct(t))})});