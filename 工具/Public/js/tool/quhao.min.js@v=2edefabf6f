function quhao(){var t=$("#tool-text").val();if(""==t)return tips_error("请输入城市名！"),$("#tool-text").focus(),!1;var s=$(".btn-success");s.attr("disabled","disabled"),tips_loading(),$.ajax({type:"post",dataType:"json",data:{city:t},url:WW_URL+"ajax",success:function(t){1==t.status?(tips_clear(),res=t.data,html="省市: "+res.name+"<br/>区号: "+res.code+"<br/>邮编: "+res.zip,$("#tool-result").html(html),$("#tool-result").show()):(tips_error(t.info),$("#tool-text").focus()),s.removeAttr("disabled","disabled")}})}