function weixin_check(){var s=$.trim($("#tool-text").val().toLowerCase().replace(/\s+/g,""));if(""==s)return tips_error("请填写要查询的网址！"),$("#tool-text").focus(),!1;var a=$(".btn-success");a.attr("disabled","disabled"),tips_loading(),$("#tool-result").hide(),$.ajax({type:"post",dataType:"json",headers:{"X-CSRF-Token":getCSRFToken()},data:{url:s},url:WW_URL+"ajax",success:function(s){if(1==s.status){tips_clear();var e="";200==s.code?e+='<span class="label label-success">正常</span> '+s.msg:201==s.code?e+='<span class="label label-warning">拦截</span> '+s.msg:e+='<span class="label label-danger">拦截</span> '+s.msg,$("#tool-result").html(e),$("#tool-result").show()}else tips_error(s.info),$("#tool-text").focus();a.removeAttr("disabled","disabled")}})}