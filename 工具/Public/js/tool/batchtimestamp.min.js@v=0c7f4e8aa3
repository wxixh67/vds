function exportCSV(t){for(var n="data:text/csv;charset=utf-8,",e=0;e<t.length;e++){n+=t[e].map(function(t){return null==t&&(t=""),t=String(t),/[",\n]/.test(t)&&(t='"'+t.replace(/"/g,'""')+'"'),t}).join(",")+"\n"}var l=document.createElement("a");l.setAttribute("href",encodeURI(n)),l.setAttribute("download","result.csv"),document.body.appendChild(l),l.click(),document.body.removeChild(l)}$(function(){function t(t){var n=$("#tool-text").val().replace(/\r\n/g,"\n").split("\n"),e=$("#unit").val(),l=$("#format").val(),o=[],a="";$("#total").html("0"),$("#tool-result").val("");for(var i=0;i<n.length;i++){var c=$.trim(n[i]);if(c){var r="";if("ts2time"===t){var s=parseInt(c,10);"s"===e&&(s*=1e3),r=dayjs(s).format(l)}else if("time2ts"===t){var u=dayjs(c);r="s"===e?Math.floor(u.valueOf()/1e3):u.valueOf()}o.push([c,r]),a+=r+"\n"}}$("#tool-result").val(a.trim("\n")),$("#total").html(o.length),$("#btnDownload").off("click").on("click",function(){exportCSV(o)})}$("#btnExample").click(function(){$("#tool-text").val("1735663959\n1735667375\n1735670268\n1735672291\n1735672880\n1735675260\n1735675777\n1735676479\n1735676609\n1735679928"),$("#btnTs2Time").trigger("click")}),$("#btnTs2Time").click(function(){t("ts2time")}),$("#btnTime2Ts").click(function(){t("time2ts")}),$("#btnReset").click(function(){$("#tool-text").val(""),$("#tool-result").val(""),$("#total").html("0")}),$(".btn-fill").click(function(){var t=$(this).data("text"),n=$(this).data("target");$("#"+n).val(t)})});