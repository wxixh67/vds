function bmi(){var e=parseFloat($("input[name='height']").val());if(isNaN(e))return tips_error("请填写身高！"),$("input[name='height']").focus(),$("input[name='height']").select(),$("#tool-result").hide(),!1;$("input[name='height']").val(e);var t=parseFloat($("input[name='weight']").val());if(isNaN(t))return tips_error("请填写体重！"),$("input[name='weight']").focus(),$("input[name='weight']").select(),$("#tool-result").hide(),!1;$("input[name='weight']").val(t);var a=(t/(e/100*(e/100))).toFixed(1),i="warning",n="消瘦";a<18.5?(i="warning",n="消瘦"):18.5<=a&&a<24?(i="success",n="正常"):24<=a&&a<27?(i="warning",n="偏胖"):27<=a&&a<30?(i="danger",n="肥胖"):30<=a&&(i="danger",n="重度肥胖");var r="";r+="BMI 数值："+a+'<br/>身体状态：<span class="text-'+i+'">'+n+"</span>",tips_clear(),$("#tool-result").show(),$("#tool-result").html(r)}