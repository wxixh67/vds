!function(){$(".exchange-panel-w");function n(){var n=this;e&&clearTimeout(e),e=setTimeout(function(){var e=n.value.trim(),o=n.name;return(o!=r||e!=i)&&(""==o?(tips_error("请选择转换前单位！"),!1):""==e?(tips_error("请填写初始数值！"),!1):(tips_loading(),void $.ajax({type:"post",dataType:"json",data:{from:o,to:"all",area:e},url:WW_URL+"ajax",success:function(n){if(1!=n.status)return tips_error(n.info),!1;tips_clear(),i=e,r=o;for(var t=n.data,a=0;a<t.length;a++)$('input[name="'+t[a].unit+'"]').val(t[a].value)}})))},500)}for(var t,a=$(".form-input"),e=null,r=null,i=null,o=0;o<a.length;o++)a[t=o].onkeydown=function(){e&&clearTimeout(e)},a[t].onkeyup=n,a[t].onchange=n,a[t].onclick=function(){$(this).select()}}();