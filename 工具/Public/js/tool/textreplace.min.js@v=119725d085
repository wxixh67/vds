function textplace(e){var t=$("#tool-text").val();if(t.length<=0)return tips_error("请输入要处理的文本！"),$("#tool-text").focus(),!1;var l=$("#text_from").val(),r=$("#text_to").val();if(0==e){l=l.replace(/[.*+?^${}()|[\]\\]/g,"\\$&");var a=new RegExp(l,"g"),o=t.replace(a,r)}else o=t.replace(new RegExp(l,"g"),r);tips_clear(),$("#tool-result").val(o)}