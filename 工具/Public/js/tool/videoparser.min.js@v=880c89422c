function video_parser(){var t=$.trim($("#tool-text").val().replace(/\s+/g,""));if(""==t)return tips_error("请输入含视频链接的文本！"),$("#tool-text").focus(),!1;var e=$(".btn-success");e.attr("disabled","disabled"),tips_loading(),$.ajax({type:"post",dataType:"json",data:{url:t},url:WW_URL+"ajax",success:function(t){if(1==t.status){tips_clear();var a='<table class="table table-bordered">';a+='<tr><td style="width: 35px;">标题</td><td>'+t.data.title+"</td></tr>",a+="<tr><td>链接</td><td>"+t.data.url+'       <a class="link btn-copy" data-clipboard-text="'+t.data.url+'"><br/>[复制]</a></td></tr>',a+="</table>",$("#tool-output").html(a),$("#tool-output").show()}else tips_error(t.info),$("#tool-text").focus();e.removeAttr("disabled","disabled")}})}