function table_search(){var e=$.trim($("#tool-keyword").val());search_table($("#search-table"),e)}function table_search_reset(){$("#tool-keyword").val(""),search_table($("#search-table"),"")}function search_table(e,t){if(t=t&&t.replace(/\s+/g,"")){var a=new RegExp(t),s=!1,c=!1,h="";$("tr",e).each(function(){c=s=!1,$("td",this).each(function(){c=!0,h=$(this).text().replace(/\s+/g,""),a.test(h)&&(s=!0)}),c&&(s?$(this).show():$(this).hide())})}else $("tr",e).each(function(){$(this).show()})}