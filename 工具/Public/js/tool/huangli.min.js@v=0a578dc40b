function huangli(){var t=$("#tool-text").val();if(""==t)return tips_error("请填写您要查询的日期！"),$("#tool-text").focus(),!1;var a=$(".btn-success");a.attr("disabled","disabled"),tips_loading(),$.ajax({type:"post",dataType:"json",data:{date:t},url:WW_URL+"ajax",success:function(t){1==t.status?(tips_clear(),$("#tool-result").show(),$("#tool-result").html(t.data)):($("#tool-result").hide(),tips_error(t.info)),a.removeAttr("disabled","disabled")}})}$("#tool-text").cxCalendar({startDate:"2017-01-01",endDate:"2027-12-31"});