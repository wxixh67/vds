function ajax_go_page(t){$("#ajax_list").html('<i class="icon-spin5 animate-spin"></i>'),$.ajax({type:"GET",url:"/home/feedback/?p="+t,timeout:5e3,success:function(t){$("#ajax_list").html(t),$("html,body").animate({scrollTop:$("#ajax_list").offset().top},100)},error:function(t,e){$("#ajax_list").html('<p class="alert alert-error hide">请求失败！请刷新页面重试。</p>'),$("html,body").animate({scrollTop:$("#ajax_list").offset().top},100)}})}function submit_feedback(){var e=$.trim($('input[name="name"]').val()),a=$.trim($('input[name="email"]').val()),n=$.trim($('input[name="referer_url"]').val()),r=$.trim($('textarea[name="content"]').val()),i=$("input[name='is_open']:checked").val();if(0==r.length)return tips_error("请输入反馈内容!"),$('textarea[name="content"]').focus(),!1;tips_loading(),new TencentCaptcha("2089782076",function(t){if(0!==t.ret)return tips_error("验证失败！"),!1;$.ajax({type:"post",url:WW_APP+"Feedback/ajax_add",data:{name:e,email:a,content:r,is_open:i,referer_url:n,ticket:t.ticket,randstr:t.randstr},dataType:"json",success:function(t){if(1!=t.status)return tips_error(t.info),!1;$('textarea[name="content"]').val(""),tips_success(t.info)}})}).show()}