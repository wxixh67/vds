function nongli(){var r=$("select[name='type']").val(),e=$("input[name='year']").val(),a=$("input[name='month']").val(),s=$("input[name='day']").val();if(""==e)return tips_error("请填写年份！"),$("input[name='year']").focus(),!1;if(""==a)return tips_error("请填写月份！"),$("input[name='month']").focus(),!1;if(""==s)return tips_error("请填写日！"),$("input[name='day']").focus(),!1;var t=$(".btn-success");t.attr("disabled","disabled"),tips_loading(),$.ajax({type:"post",dataType:"json",data:{type:r,year:e,month:a,day:s},url:WW_URL+"ajax",success:function(r){1==r.status?(tips_clear(),res=r.data,$("#tool-result").show(),$("#tool-result").html('<img src="'+WW_PUBLIC+"js/tool/shengxiao/"+res.sx+'.gif" /><p>农历：'+res.lunar+" "+res.ganzhi+" "+res.lunar_date+"<br/>生肖："+res.sx+"<br/>公历："+res.solar+"<br/>星座："+res.xz+"<br/>今年生日的公历日期："+res.this_year_lunar_solar+"<br/>明年生日的公历日期："+res.next_year_lunar_solar+"</p>")):($("#tool-result").hide(),tips_error(r.info)),t.removeAttr("disabled","disabled")}})}