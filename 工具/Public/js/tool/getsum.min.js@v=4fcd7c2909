function get_sum(){var t=$("#tool-text").val(),e=$("#split_format").val();if(""==t)return tips_error("请填写需要求和的内容！"),$("#tool-text").focus(),!1;if(2e3<t.length)return tips_error("输入字符长度不能超过2000！"),$("#tool-text").focus(),!1;var s=$(".btn-success");s.attr("disabled","disabled"),tips_loading(),$.ajax({type:"post",dataType:"json",data:{numbers:t,delimiter:e},url:WW_URL+"ajax",success:function(t){1==t.status?(tips_clear(),$("#tool-result").html(t.data).show()):(tips_error(t.info),$("#tool-text").focus()),s.removeAttr("disabled","disabled")}})}